{
  "name": "ele-vue3-natv",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/entry.js",
  "author": {
    "name": "echo"
  },
  "description": "一个element+vue+antv项目",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "start": "electron ."
  },
  "build": {
    "appId": "com.electron.desktop",
    "productName": "electron",
    "asar": true,
    "copyright": "Copyright © 2022 electron",
    "directories": {
      "output": "release/"
    },
    "files": [
      "dist",
      "dist-electron"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080"
      }
    ],
    "releaseInfo": {
      "releaseNotes": "版本更新的具体内容"
    }
  },
  "dependencies": {
    "@antv/g": "^5.18.21",
    "@antv/g-canvas": "^1.11.24",
    "@antv/g-svg": "^1.10.23",
    "@antv/g-webgl": "^1.9.34",
    "@antv/g6": "^4.8.23",
    "@antv/hierarchy": "^0.6.11",
    "@antv/layout": "^0.3.24",
    "@antv/x6": "^2.16.1",
    "@antv/x6-plugin-keyboard": "^2.2.1",
    "@antv/x6-plugin-selection": "^2.2.1",
    "@antv/x6-vue-shape": "^2.1.1",
    "mitt": "^3.0.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.8",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "electron": "^27.1.2",
    "electron-builder": "^24.6.4",
    "tree-kill": "^1.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.15.4",
    "vue-tsc": "^1.8.22"
  }
}
